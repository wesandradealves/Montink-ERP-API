{
    "openapi": "3.0.0",
    "info": {
        "title": "Montink ERP API",
        "description": "API para sistema Mini ERP Montink com gestão de produtos, pedidos, cupons e estoque",
        "contact": {
            "email": "admin@montink.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost",
            "description": "Servidor de Desenvolvimento"
        }
    ],
    "paths": {
        "/api/health": {
            "get": {
                "tags": [
                    "Health"
                ],
                "summary": "Verificação de saúde da API",
                "description": "Endpoint para verificar se a API está funcionando corretamente",
                "operationId": "healthCheck",
                "responses": {
                    "200": {
                        "description": "API funcionando corretamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HealthCheck"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/address/cep/{cep}": {
            "get": {
                "tags": [
                    "Address"
                ],
                "summary": "Buscar endereço por CEP",
                "description": "Consulta endereço através da API ViaCEP",
                "operationId": "getAddressByCep",
                "parameters": [
                    {
                        "name": "cep",
                        "in": "path",
                        "description": "CEP para consulta (8 dígitos)",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "pattern": "^[0-9]{8}$",
                            "example": "01310100"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Endereço encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/Address"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Endereço encontrado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "CEP não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "CEP não encontrado"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "CEP inválido",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "CEP deve conter 8 dígitos"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/address/validate-cep": {
            "post": {
                "tags": [
                    "Address"
                ],
                "summary": "Validar CEP",
                "description": "Valida se um CEP existe na base ViaCEP",
                "operationId": "validateCep",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "cep"
                                ],
                                "properties": {
                                    "cep": {
                                        "description": "CEP para validação",
                                        "type": "string",
                                        "example": "01310100"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Resultado da validação",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "valid": {
                                                    "type": "boolean",
                                                    "example": true
                                                },
                                                "cep": {
                                                    "type": "string",
                                                    "example": "01310100"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "CEP validado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/cart": {
            "get": {
                "tags": [
                    "Cart"
                ],
                "summary": "Obter carrinho atual",
                "operationId": "e1bc6d03f0b8ceb524a277008581b4c9",
                "responses": {
                    "200": {
                        "description": "Carrinho retornado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "properties": {
                                                "items": {
                                                    "type": "array",
                                                    "items": {
                                                        "$ref": "#/components/schemas/CartItem"
                                                    }
                                                },
                                                "subtotal": {
                                                    "type": "number",
                                                    "example": 199.98
                                                },
                                                "total_items": {
                                                    "type": "integer",
                                                    "example": 2
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Cart"
                ],
                "summary": "Adicionar produto ao carrinho",
                "operationId": "7856fec4cde2bf1409b38b36d6edf2ff",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "product_id",
                                    "quantity"
                                ],
                                "properties": {
                                    "product_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "quantity": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "variations": {
                                        "type": "object",
                                        "example": {
                                            "size": "M",
                                            "color": "blue"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Produto adicionado ao carrinho",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Cart"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erro de validação ou estoque insuficiente"
                    }
                }
            },
            "delete": {
                "tags": [
                    "Cart"
                ],
                "summary": "Limpar carrinho",
                "operationId": "3b3b5bfe36351657211aec2e597b36ad",
                "responses": {
                    "200": {
                        "description": "Carrinho limpo com sucesso"
                    }
                }
            }
        },
        "/api/cart/{id}": {
            "delete": {
                "tags": [
                    "Cart"
                ],
                "summary": "Remover item do carrinho",
                "operationId": "568d7ed8881e9e40b6e83a6e6df16384",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Item removido com sucesso"
                    },
                    "404": {
                        "description": "Item não encontrado"
                    }
                }
            },
            "patch": {
                "tags": [
                    "Cart"
                ],
                "summary": "Atualizar quantidade de item do carrinho",
                "operationId": "e53e741fac97b1862e473759b244a09f",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "quantity"
                                ],
                                "properties": {
                                    "quantity": {
                                        "type": "integer",
                                        "example": 3
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Quantidade atualizada com sucesso"
                    },
                    "400": {
                        "description": "Erro de validação ou estoque insuficiente"
                    }
                }
            }
        },
        "/api/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Listar produtos",
                "description": "Retorna lista de produtos com opção de filtros",
                "operationId": "getProducts",
                "parameters": [
                    {
                        "name": "only_active",
                        "in": "query",
                        "description": "Filtrar apenas produtos ativos",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Buscar por nome ou SKU",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de produtos retornada com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Product"
                                            }
                                        },
                                        "meta": {
                                            "properties": {
                                                "total": {
                                                    "type": "integer",
                                                    "example": 10
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Products"
                ],
                "summary": "Criar novo produto",
                "description": "Cria um novo produto no sistema",
                "operationId": "createProduct",
                "requestBody": {
                    "description": "Dados do produto",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "price",
                                    "sku"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Notebook Dell"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Notebook Dell Inspiron 15"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "minimum": 0,
                                        "example": 2999.9
                                    },
                                    "sku": {
                                        "type": "string",
                                        "maxLength": 50,
                                        "example": "NTB-DELL-001"
                                    },
                                    "active": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "variations": {
                                        "type": "object",
                                        "example": {
                                            "cor": "preto",
                                            "tamanho": "15 polegadas"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Produto criado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/Product"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Produto criado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erro de validação",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "SKU já existe no sistema"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/products/{id}": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Buscar produto por ID",
                "description": "Retorna um produto específico pelo ID",
                "operationId": "getProductById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do produto",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Produto encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/Product"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Produto encontrado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Produto não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Produto não encontrado"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "Products"
                ],
                "summary": "Excluir produto",
                "description": "Remove um produto do sistema",
                "operationId": "deleteProduct",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do produto",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Produto excluído com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "null"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Produto excluído com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Produto não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Produto não encontrado"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "tags": [
                    "Products"
                ],
                "summary": "Atualizar produto",
                "description": "Atualiza um produto existente",
                "operationId": "updateProduct",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID do produto",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Dados do produto para atualização",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "maxLength": 255,
                                        "example": "Notebook Dell Atualizado"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Notebook Dell Inspiron 15 - Nova versão"
                                    },
                                    "price": {
                                        "type": "number",
                                        "format": "float",
                                        "minimum": 0,
                                        "example": 3299.9
                                    },
                                    "sku": {
                                        "type": "string",
                                        "maxLength": 50,
                                        "example": "NTB-DELL-001-V2"
                                    },
                                    "active": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "variations": {
                                        "type": "object",
                                        "example": {
                                            "cor": "preto",
                                            "tamanho": "15 polegadas",
                                            "ram": "16GB"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Produto atualizado com sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/Product"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Produto atualizado com sucesso"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Produto não encontrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Produto não encontrado"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erro de validação",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "SKU já existe no sistema"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Product": {
                "title": "Product",
                "description": "Modelo de Produto",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Notebook Dell"
                    },
                    "description": {
                        "type": "string",
                        "example": "Notebook Dell Inspiron 15"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 2999.9
                    },
                    "sku": {
                        "type": "string",
                        "example": "NTB-DELL-001"
                    },
                    "active": {
                        "type": "boolean",
                        "example": true
                    },
                    "variations": {
                        "type": "object",
                        "example": {
                            "cor": "preto",
                            "tamanho": "15 polegadas"
                        }
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00.000000Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00.000000Z"
                    }
                },
                "type": "object"
            },
            "ApiResponse": {
                "title": "API Response",
                "description": "Formato padrão de resposta da API",
                "properties": {
                    "data": {
                        "description": "Dados da resposta",
                        "type": "object"
                    },
                    "message": {
                        "type": "string",
                        "example": "Operação realizada com sucesso"
                    }
                },
                "type": "object"
            },
            "ApiListResponse": {
                "title": "API List Response",
                "description": "Formato padrão de resposta de listagem",
                "properties": {
                    "data": {
                        "description": "Lista de itens",
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "meta": {
                        "properties": {
                            "total": {
                                "type": "integer",
                                "example": 10
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "ApiErrorResponse": {
                "title": "API Error Response",
                "description": "Formato padrão de resposta de erro",
                "properties": {
                    "error": {
                        "type": "string",
                        "example": "Erro ao processar solicitação"
                    }
                },
                "type": "object"
            },
            "ValidationError": {
                "title": "Validation Error",
                "description": "Resposta de erro de validação",
                "properties": {
                    "message": {
                        "type": "string",
                        "example": "Os dados fornecidos são inválidos"
                    },
                    "errors": {
                        "description": "Erros de validação por campo",
                        "properties": {
                            "name": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "example": "O campo nome é obrigatório"
                                }
                            },
                            "price": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "example": "O campo preço deve ser numérico"
                                }
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "Order": {
                "title": "Order",
                "description": "Modelo de Pedido",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "customer_name": {
                        "type": "string",
                        "example": "João Silva"
                    },
                    "customer_email": {
                        "type": "string",
                        "format": "email",
                        "example": "joao@email.com"
                    },
                    "customer_phone": {
                        "type": "string",
                        "example": "(11) 98765-4321"
                    },
                    "customer_cpf": {
                        "type": "string",
                        "example": "123.456.789-00"
                    },
                    "subtotal": {
                        "type": "number",
                        "format": "float",
                        "example": 100
                    },
                    "discount": {
                        "type": "number",
                        "format": "float",
                        "example": 10
                    },
                    "shipping_cost": {
                        "type": "number",
                        "format": "float",
                        "example": 20
                    },
                    "total": {
                        "type": "number",
                        "format": "float",
                        "example": 110
                    },
                    "status": {
                        "type": "string",
                        "enum": [
                            "pending",
                            "processing",
                            "shipped",
                            "delivered",
                            "cancelled"
                        ],
                        "example": "pending"
                    },
                    "coupon_code": {
                        "type": "string",
                        "example": "DESC10"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00.000000Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-01-01T00:00:00.000000Z"
                    }
                },
                "type": "object"
            },
            "Coupon": {
                "title": "Coupon",
                "description": "Modelo de Cupom",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "code": {
                        "type": "string",
                        "example": "DESC10"
                    },
                    "description": {
                        "type": "string",
                        "example": "10% de desconto"
                    },
                    "type": {
                        "type": "string",
                        "enum": [
                            "fixed",
                            "percentage"
                        ],
                        "example": "percentage"
                    },
                    "value": {
                        "type": "number",
                        "format": "float",
                        "example": 10
                    },
                    "minimum_value": {
                        "type": "number",
                        "format": "float",
                        "example": 50
                    },
                    "usage_limit": {
                        "type": "integer",
                        "example": 100
                    },
                    "used_count": {
                        "type": "integer",
                        "example": 10
                    },
                    "valid_from": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-01-01"
                    },
                    "valid_until": {
